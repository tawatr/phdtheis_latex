doi = {10.1016/j.cageo.2004.09.014},
file = {:Users/puwis/Documents/Mendeley/Singh, Tiwari, Singh\_2005\_Computers \& Geosciences\_One-dimensional inversion of geo-electrical resistivity sounding data using artifici.pdf:pdf},
