month = sep,
number = {C},
pages = {64--75},
publisher = {Elsevier},
title = {{Thailand's crustal properties from tele-seismic receiver function studies}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84927579516\&partnerID=tZOtx3y1},
volume = {632},
year = {2014}
}
