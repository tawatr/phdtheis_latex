@article{Yaramanci1999,
abstract = {The surface nuclear magnetic resonance (SNMR) method has been tested at$\backslash$na site in Haldensleben, northern Germany, to assess the suitability of$\backslash$nthis new method for groundwater exploration and environmental$\backslash$ninvestigations. More information is obtained by SNMR, particularly with$\backslash$nrespect to aquifer parameters, than with other geophysical techniques.$\backslash$nSNMR measurements were carried out at three borehole locations, together$\backslash$nwith 2D and 1D direct current geoelectrics, as well as$\backslash$nground-penetrating radar, and well logging (induction log, gamma-ray log$\backslash$nand pulsed neutron-gamma log). Permeabilities were calculated from the$\backslash$ngrain-size distributions of core material determined in the laboratory.$\backslash$nIt is demonstrated that the SNMR method is able to detect groundwater$\backslash$nand the results are in good agreement with other geophysical and$\backslash$nhydrogeological data. Using the SNMR method, the water content of the$\backslash$nunsaturated and saturated zones (i.e. porosity of an aquifer) can be$\backslash$nreliably determined. This information and resistivity data permit in$\backslash$nsitu determination of other aquifer parameters. Comparison of the SNMR$\backslash$nresults with borehole data clearly shows that the water content$\backslash$ndetermined by SNMR is the free or mobile water in the pores. The$\backslash$npermeabilities estimated from the SNMR decay times are similar to those$\backslash$nderived from sieve analysis of core material. Thus, the combination of$\backslash$nSNMR with geoelectric methods promises to be a powerful tool for$\backslash$nstudying aquifer properties.},
author = {Yaramanci, Ugur and Lange, Gerhard and Kn\"{o}del, Klaus},
doi = {10.1046/j.1365-2478.1999.00161.x},
file = {:Users/puwis/Documents/Mendeley/Yaramanci, Lange, Kn\"{o}del\_1999\_Geophysical Prospecting\_Surface NMR within a geophysical study of an aquifer at Haldensleben (Germany).pdf:pdf},
issn = {00168025},
journal = {Geophysical Prospecting},
number = {6},
pages = {923--943},
title = {{Surface NMR within a geophysical study of an aquifer at Haldensleben (Germany)}},
volume = {47},
year = {1999}
}
