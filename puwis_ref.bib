doi = {10.1016/S1474-7065(03)00069-X},
file = {:Users/puwis/Documents/Mendeley/Vosteen, Schellschmidt\_2003\_Physics and Chemistry of the Earth, Parts ABC\_Influence of temperature on thermal conductivity, thermal capa.pdf:pdf},
issn = {14747065},
journal = {Physics and Chemistry of the Earth, Parts A/B/C},
keywords = {geothermics,rock physics,temperature dependence,thermal capacity,thermal conductivity,thermal diffusivity},
number = {9-11},
pages = {499--509},
title = {{Influence of temperature on thermal conductivity, thermal capacity and thermal diffusivity for different types of rock}},
