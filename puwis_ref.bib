@misc{Groom1989,
abstract = {There are many occasions on which the magnetotelluric impedance tensor$\backslash$nis affected by local galvanic distortion (channeling) of electric$\backslash$ncurrents arising from induction in a conductive structure which is$\backslash$napproximately two-dimensional (2-D) on a regional scale. Even though$\backslash$nthe inductive behavior is 2-D, the resulting impedance tensor can$\backslash$nbe shown to have three-dimensional (3-D) behavior. Conventional procedures$\backslash$nfor rotating the impedance tensor such as minimizing the mean square$\backslash$nmodulus of the diagonal elements do not in general recover the principal$\backslash$naxes of induction and thus do not recover the correct principal impedances$\backslash$nbut rather linear combinations of them. This paper presents a decomposition$\backslash$nof the impedance tensor which separates the effects of 3-D channeling$\backslash$nfrom those of 2-D induction. Where the impedance tensor is actually$\backslash$nthe result of regional 1-D or 2-D induction coupled with local frequency$\backslash$nindependent telluric distortion, the method correctly recovers the$\backslash$nprincipal axes of induction and, except for a static shift (multiplication$\backslash$nby a frequency independent real constant), the two principal impedances.$\backslash$nAlso obtained are two parameters (twist and shear), which partially$\backslash$ndescribe the effects of telluric distortion. It is shown that the$\backslash$ntensor operator which describes the telluric distortions can always$\backslash$nbe factored into the product of three tensor sub-operators (twist,$\backslash$nshear, local anisotropy) and a scalar. This product factorization$\backslash$nallows assimilation of local anisotropy, if present, into the regional$\backslash$nanisotropy. The method of decomposition is given in the paper along$\backslash$nwith a discussion of the improvements obtained over the conventional$\backslash$nmethod and an example with real data.},
author = {Groom, Ross W and Bailey, Richard C},
booktitle = {Journal of Geophysical Research},
doi = {10.1029/JB094iB02p01913},
file = {:Users/puwis/Documents/Mendeley/Groom, Bailey\_1989\_Journal of Geophysical Research\_Decomposition of magnetotelluric impedance tensors in the presence of local three-dim.pdf:pdf},
isbn = {0148-0227},
issn = {0148-0227},
pages = {1913},
title = {{Decomposition of magnetotelluric impedance tensors in the presence of local three-dimensional galvanic distortion}},
volume = {94},
year = {1989}
}
@article{Groom1992,
abstract = {This paper primarily examines the effects of small-scale or near-surface conductivity inhomogeneities on the magnetotelluric (MT) impedance tensor. These effects cause three different types of distortion results. (1) The well-known static shifts of sounding curves. (2) When the underlying regional setting is two-dimensional then the two regional impedances are mixed in an arbitrary coordinate system. Thus the level and shape of each sounding curve is distorted as are the phases. (3) At sufficiently high frequencies these effects generate anomalous magnetic fields that in turn alter the background phases. This tutorial first explores the usefulness of various MT tensor analysis techniques to overcome the problem of phase mixing and to recover regional information in the presence of local geological noise. Synthetic and experimental data are considered. A sequence of a priori models of increasing complexity are described. The use of appropriate decompositions of the MT tensor each with an increasing number of parameters is emphasised. In a second part, phase mixing and static shifts are examined from a synoptic view. Some static shift removal techniques that can be used in conjunction with the decomposition are discussed. This paper is not a review but rather an investigation of a few methods that the authors have found useful with field data.},
author = {Groom, Ross W. and Bahr, Karsten},
doi = {10.1007/BF01903483},
file = {:Users/puwis/Documents/Mendeley/Groom, Bahr\_1992\_Surveys in Geophysics\_Corrections for near surface effects Decomposition of the magnetotelluric impedance tensor and sc.pdf:pdf},
isbn = {0169-3298},
issn = {01693298},
journal = {Surveys in Geophysics},
number = {4-5},
pages = {341--379},
title = {{Corrections for near surface effects: Decomposition of the magnetotelluric impedance tensor and scaling corrections for regional resistivities: A tutorial}},
volume = {13},
year = {1992}
}
