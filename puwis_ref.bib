@article{Smith2014,
abstract = {In the period from 2008 to 2012, the topic of electromagnetic (EM) induction methods applied to mineral exploration has been the subject of more than 50 papers in journals and more than 300 extended abstracts presented at conferences (about 100 of which contain developments worthy of mentioning). Most of the work at the universities has been on modelling, inversion and data processing, and most of this material is pub- lished in the refereed literature. However, academia has also undertaken work on system geometry changes, system calibration and sensor design. There have been papers describing new systems developed for mineral exploration and case histories describing the use of EM methods to directly discover mineral deposits or to map the geology. Most of this work is by the service companies and mining companies and reported in the unrefereed literature. Since 2008, the pace of development of helicopter time-domain systems has slowed and more effort has been directed to developing natural source magnetic systems and to modelling and inverting this data. A number of studies comparing the results from natural source methods with the results from artificial source methods conclude that the natural source methods can see large-scale geological structures usually when there is a weak conductivity contrast with the surrounding material, but the natural source methods are unable to see small features that have a very large conductivity contrast with the country rock. Hence, they are not a good detector of mineral deposits unless one is looking for a large porphyry system.},
author = {Smith, Richard},
doi = {10.1007/s10712-013-9227-1},
file = {:Users/puwis/Documents/Mendeley/Smith\_2014\_Surveys in Geophysics\_Electromagnetic Induction Methods in Mining Geophysics from 2008 to 2012.pdf:pdf},
issn = {01693298},
journal = {Surveys in Geophysics},
keywords = {Controlled source,Electromagnetic induction,Mineral exploration,Mining,Natural field},
number = {1},
pages = {123--156},
title = {{Electromagnetic Induction Methods in Mining Geophysics from 2008 to 2012}},
volume = {35},
year = {2014}
}
@article{Song2002,
abstract = {We have applied self-potential (SP) survey and long- term SP monitoring along with pumping test to delineate hydrology and to monitor deep ground water movement around test drill wells in Pohang geothermal development site, Korea. Before drilling the test wells, SP survey was carried out to figure out overall potential distribution of the site. The pumping test was performed in April 2004 for 72 hours. SP monitoring started several days before the pumping tests water with 128-channel automatic recording system. SP survey showed positive anomaly at north of the borehole, which may be an up-flow zone of the deep geothermal due to electrokinetic potential generated by hydrothermal circulation. SP monitoring during pumping test has been performed to figure out the reservoir variation and direction of fluid flow. It was not easy to see clear variations of SP according to pumping on and off since the area is covered with some 360 m- thick tertiary sediments of electrical resistivity lower than 10 ohm-m, which resulted in serious attenuation of the electrokinetic potential induced by deep groundwater flow. However, when we compared the variation of SP with those of groundwater pumped water, we could identify some parts responding to pumping. Dominant changes level and temperature of were observed in southwest of the pumping well, in which three- dimensional (3-D) magnetotelluric (MT) survey results shows low-resistivity anomaly in the depth region of 600 m-1,000 m. Overall analysis suggests that there exist hydraulic connection through the southwestern part to pumping well.},
author = {Song, Yoonho and Lim, Seong-keun and Lee, Tae Jong and Yasukawa, Kasumi and Song, Sung-ho},
file = {:Users/puwis/Documents/Mendeley/Song et al.\_2002\_\_Application of SP survey to low-temperature geothermal development in.pdf:pdf},
journal = {?},
number = {1},
title = {{Application of SP survey to low-temperature geothermal development in}},
year = {2002}
}
