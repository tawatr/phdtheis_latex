keywords = {Mae Kuang Fault,Strike-slip faults,Tectonics},
number = {1},
pages = {79--89},
publisher = {Elsevier Ltd},
title = {{Kinematics and tectonic implications of the Mae Kuang fault, northern Thailand}},
url = {http://dx.doi.org/10.1016/j.jseaes.2003.09.008},
volume = {24},
year = {2004}
